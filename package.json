{
  "name": "react-redux-ts",
  "version": "1.2.0",
  "description": "Redux, react-redux workflow simplifier",
  "author": "elrix",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/eilrix/react-redux-ts"
  },
  "homepage": "https://github.com/eilrix/react-redux-ts#readme",
  "main": "lib/redux.js",
  "module": "es/redux.js",
  "types": "dist/index.d.ts",
  "unpkg": "dist/index.js",
  "npmFileMap": [
    {
      "basePath": "/dist/",
      "files": [
        "*.js"
      ]
    }
  ],
  "files": [
    "dist",
    "lib",
    "es",
    "src"
  ],
  "dependencies": {
    "@babel/runtime": "^7.9.2",
    "react-redux": "^7.1.3",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/plugin-external-helpers": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.5",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-flow": "^7.9.0",
    "@babel/preset-typescript": "^7.9.0",
    "@babel/register": "^7.9.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@rollup/plugin-replace": "^2.3.2",
    "@types/react-redux": "^7.1.5",
    "@types/redux": "^3.6.0",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "eslint": "^7.1.0",
    "rollup": "^2.7.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-typescript2": "^0.27.0",
    "terser": "^4.7.0",
    "typescript": "^3.9.3"
  },
  "scripts": {
    "lint": "eslint . --ext .ts",
    "build": "tsc && npx terser --output ./dist/index.min.js --compress --mangle -- ./dist/index.js",
    "build2": "rollup -c"
  },
  "keywords": [
    "redux",
    "react",
    "react-redux",
    "typescript",
    "type",
    "redux-type"
  ]
}
